<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
	<!-- this means that files with a .dog extension are recognized by this loader -->
        <ext name="dog"/>
        <resolver mime="text/x-dog"/>
    </file>
</MIME-resolver>
